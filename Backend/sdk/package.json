{
  "name": "@celo-ai/sdk",
  "version": "1.0.0",
  "description": "Celo AI Automation Engine SDK - Multi-chain blockchain automation with AI agents",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./autofi": {
      "import": "./dist/sdk/autofi-sdk.js",
      "require": "./dist/sdk/autofi-sdk.js",
      "types": "./dist/sdk/autofi-sdk.d.ts"
    },
    "./modules": {
      "import": "./dist/modules/index.js",
      "require": "./dist/modules/index.js",
      "types": "./dist/modules/index.d.ts"
    },
    "./security": {
      "import": "./dist/utils/security.js",
      "require": "./dist/utils/security.js",
      "types": "./dist/utils/security.d.ts"
    },
    "./masking": {
      "import": "./dist/utils/data-masker.js",
      "require": "./dist/utils/data-masker.js",
      "types": "./dist/utils/data-masker.d.ts"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "celo-ai": "./cli/index.js"
  },
  "files": [
    "dist",
    "cli",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:webpack": "webpack --mode production",
    "build:all": "npm run build && npm run build:webpack",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "npm audit --audit-level=moderate",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm test && npm run test:security",
    "prepack": "npm run build",
    "docs": "typedoc --options typedoc.json",
    "clean": "rimraf dist docs",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "audit:production": "npm audit --production",
    "security:check": "npm audit --audit-level=high",
    "postinstall": "npm run security:check || true"
  },
  "keywords": [
    "celo",
    "blockchain",
    "ai",
    "automation",
    "multi-chain",
    "defi",
    "nft",
    "smart-contracts",
    "ethereum",
    "polygon",
    "bsc",
    "arbitrum",
    "optimism"
  ],
  "author": {
    "name": "Celo AI Team",
    "email": "dev@celo-ai.com",
    "url": "https://celo-ai.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/celo-ai/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/celo-ai/sdk/issues"
  },
  "homepage": "https://github.com/celo-ai/sdk#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "viem": "^2.21.0",
    "axios": "^1.6.0",
    "events": "^3.3.0",
    "better-sqlite3": "^9.2.2",
    "@google/generative-ai": "^0.2.1",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "commander": "^11.1.0"
  },
  "optionalDependencies": {
    "winston": "^3.11.0",
    "pino": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@types/winston": "^2.4.4",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.25.0",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "ts-loader": "^9.5.1",
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-typescript": "^7.23.5",
    "babel-loader": "^9.1.3"
  },
  "peerDependencies": {
    "viem": "^2.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
