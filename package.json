{
  "name": "celo-automator",
  "version": "2.0.0",
  "description": "Advanced AI-powered Celo blockchain automation monorepo with LangChain orchestration",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rimraf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "turbo run type-check",
    "deploy": "turbo run deploy"
  },
  "keywords": [
    "celo",
    "blockchain",
    "ai",
    "automation",
    "langchain",
    "monorepo",
    "turborepo",
    "defi",
    "smart-contracts",
    "workflow"
  ],
  "author": "Celo AI Agents Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "rimraf": "^6.1.0",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  },
  "repository": {    "type": "git",
    "url": "https://github.com/celo-ai-agents/celo-automator"
  },
  "bugs": {
    "url": "https://github.com/celo-ai-agents/celo-automator/issues"
  },
  "homepage": "https://github.com/celo-ai-agents/celo-automator#readme"
}
